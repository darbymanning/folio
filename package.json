    "@tailwindcss/vite": "4.0.0-alpha.17",
    "@types/eslint": "8.56.10",
    "@types/node": "^20.14.10",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "7.16.0",
    "@typescript-eslint/parser": "7.16.0",
    "clsx": "2.1.1",
    "dotenv": "16.4.5",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
